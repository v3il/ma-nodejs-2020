        "browser": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "no-use-before-define": "off",
        "no-alert": "off"
    }
}
